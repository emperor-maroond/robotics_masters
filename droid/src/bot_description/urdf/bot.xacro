<!-- Links to constrain to planar -->
<link name="world"/>

<link name="y_axis_link">
    <collision>
        <origin xyz="0 0 1" rpy="0 0 0"/>
        <geometry>
            <box size="0.01 0.01 0.01"/>
        </geometry>
    </collision>
    <visual>
        <origin xyz="0 0 1" rpy="0 0 0"/>
        <geometry>
            <box size="0.01 0.01 0.01"/>
        </geometry>
        <material name="silver"/>
    </visual>
    <inertial>
        <origin xyz="0 0 1" rpy="0 0 0"/>
        <mass value="0.01"/>
        <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
    </inertial>
</link>

<joint name="y_direction" type="prismatic">
    <parent link="world"/>
    <child link="y_axis_link"/>
    <axis xyz="0 1 0"/>
    <limit effort="100" lower="-1.79769e+308" upper="1.79769e+308" velocity="100"/>
</joint>

<joint name="z_direction" type="prismatic">
    <parent link="y_axis_link"/>
    <child link="base_link"/>
    <axis xyz="0 0 1"/>
    <limit effort="100" lower="-1.79769e+308" upper="1.79769e+308" velocity="100"/>
</joint>

</robot>
