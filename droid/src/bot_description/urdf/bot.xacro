    <origin rpy="0 0 0" xyz="0.02415 1e-06 -0.27"/>
    <parent link="servo_arm_1_1_1"/>
    <child link="dsnu-20-200-pps-a_03_1_1_1"/>
    <axis xyz="0.0 0.0 -1.0"/>
    <limit effort="200" lower="0.0" upper="0.19" velocity="500"/>
