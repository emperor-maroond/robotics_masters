<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(find gazebo_ros)/worlds/empty.world"/>
        <arg name="paused" value="true"/>
    </include>
        
    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-file $(find twoleg_sim)/urdf/biped.urdf -urdf -model biped -z 2" />
    
    <include file="$(find twoleg_sim)/launch/biped_control.launch"/>

</launch>